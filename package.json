{
  "name": "base-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/src/main.js",
    "dev": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --respawn src/main.ts",
    "migrate:make": "knex --knexfile src/engine/database/knexfile.ts migrate:make",
    "migrate:latest": "knex --knexfile src/engine/database/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/engine/database/knexfile.ts migrate:rollback",
    "seed:run": "knex --knexfile src/engine/database/knexfile.ts seed:run",
    "create:use-case": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/script/commands/create-use-case/create-use-case.script.ts",
    "test": "jest"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@overnightjs/core": "^1.7.6",
    "@types/mustache": "^4.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "environment-variables-decorator": "^0.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "knex": "^2.4.2",
    "mustache": "^4.2.0",
    "pg": "^8.8.0",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.11.10",
    "@types/readline-sync": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.1",
    "tsc-alias": "^1.6.1",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
